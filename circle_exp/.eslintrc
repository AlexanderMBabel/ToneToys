{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 11,
    "ecmaFeatures": {
      "arrowFunctions": true
    }
  },
  "extends": ["airbnb"],
  "plugins": ["eslint-plugin-prettier"],
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "comma-dangle": ["error", "never"],
    "no-path-concat": "off",
    "no-console": "off",
    "no-undef": "off"
  }
}
